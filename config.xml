<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sample.contactcard" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Tenet ContactCard</name>
    <description>Contact card sample app for Tenet Health.</description>
    <author email="absachdeva@gmail.com">Abhinav Sachdeva</author>
    <content src="index.html" />
    <access origin="*" launch-external="yes"/>	
	<gap:plugin name="cordova-plugin-whitelist" spec="1.3.2" source="npm" />
	<plugin name="cordova-plugin-inappbrowser" spec="1.7.1" source="npm" />
<!--    <gap:plugin name="phonegap-plugin-push" spec="1.9.0" source="npm" />-->
    <plugin name="phonegap-plugin-push" spec="1.9.0" source="npm">
        <param name="SENDER_ID" value="848679895857" />
    </plugin>
    
    <allow-intent href="tel:*" />
	<allow-intent href="http:*" />
	<allow-intent href="https:*" />
    
    
	<preference name="stay-in-webview" value="false" />
	<preference name="orientation" value="portrait" />   
    <preference name="android-installLocation" value="auto" />
	
    <icon src="images/favicon-32x32.png" gap:platform="android" gap:qualifier="mdpi" />
    <icon src="images/favicon-96x96.png"   			gap:platform="android" gap:qualifier="xhdpi" />
</widget>
